<layout>
    <!-- Define available containers -->
    <define>
        <containers>
            <container key="no-layout" tpl="fox/no-layout"/>
            <container key="one-column" tpl="fox/one-column"/>
            <container key="two-columns-left" tpl="fox/two-columns-left"/>
            <container key="two-columns-right" tpl="fox/two-columns-right"/>
            <container key="three-columns" tpl="fox/three-columns"/>
        </containers>
    </define>
    <container key="three-columns" />
    <view key="left" class="core/list" />
    <view key="right" class="core/list" />
    <view key="content" class="core/list" />
    <view key="head" class="admin/html/head" tpl="fox/html/head" />
    <view key="header" class="core/template" tpl="fox/html/header"/>
    <view key="navigation" class="admin/cms/navigation" tpl="fox/html/navigation"/>
    <view key="footer" class="core/template" tpl="fox/html/footer" />
    <view key="profiler" class="core/html/profiler" tpl="fox/html/profiler" />
    <ref key="content">
        <view key="messages" class="core/template" tpl="fox/html/messages"/>
    </ref>
    <head>
        <title>Zendfox Admin</title>
        <script src="theme/global/js/jquery/jquery-1.4.4.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/jquery/jquery.ui.core.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/jquery/jquery.ui.widget.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/jquery/jquery.ui.position.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/jquery/jquery.ui.mouse.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/jquery/jquery.ui.datepicker.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/jquery/jquery.ui.dialog.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/jquery/jquery.ui.button.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/jquery/jquery.effects.core.min.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="theme/global/js/debugger.js" type="text/javascript" language="javascript" _global="global"/>
        <script src="js/hoverIntent.js" type="text/javascript" language="javascript"/>
        <script src="js/navigation.js" type="text/javascript" language="javascript"/>
        <link href="css/navigation.css" type="text/css" rel="stylesheet" media="screen" />
        <link href="css/style.css" type="text/css" rel="stylesheet" />
        <link href="css/ui/jquery-ui-1.8.12.custom.css" type="text/css" rel="stylesheet" />
        <link href="css/style-ie.css" type="text/css" rel="stylesheet" if="IE" />
        <link href="css/style-ie7.css" type="text/css" rel="stylesheet" if="IE 7" />
    </head>
</layout>